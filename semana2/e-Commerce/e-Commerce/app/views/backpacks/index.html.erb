<h1>Listing backpacks</h1> 
<% @backpacks.each do |backpack|%>
    <div><%= backpack.color %> 
        <%= link_to 'Show',  backpack_path(backpack)%>
        <%= link_to 'Edit', edit_backpack_path(backpack) %>
        <%= link_to 'Destroy', backpack_path(backpack), method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
<% end %>

